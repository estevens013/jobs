<% title " - Declined Jobs" %>
<div class="content">
	<h1>Declined Jobs</h1>
	<table>
		<tr style="text-align:left;">
			<th>ID</th>
			<th>Name</th>
			<th>P.O.</th>
			<th>Customer</th>
			<th>Owner</th>
			<th>Internal Owner</th>
			<th>Status</th>
			<th>Modified</th>
			<th>Due</th>
		</tr>
		<% for job in @jobs %>
			<tr id="job_<%= job.id %>">
				<td><%= job.id %></td>
				<td><%= job.name %></td>
				<td><%= job.purchaseOrder %></td>
				<td><%= job.customer %></td>
				<td><%= job.owner %></td>
				<td><%= job.internalOwner %></td>
				<td><%= job.status %></td>
				<td><%= job.modified %></td>
				<td><%= job.due %></td>
			</tr>
		<% end %>
	</table>
	<br /><br />
</div>